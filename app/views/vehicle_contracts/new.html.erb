<%= render(:partial => 'layouts/left') %>
<div id="main">
  <div class="container-fluid">
    <%= render(:partial => 'layouts/notice') %>
    <div class="page-header">
      <div class="pull-left">
        <h1>New Vehicle Contract</h1>
      </div>
      <div class="pull-right">
        <br>
        <%= link_to '<i class="icon-undo"></i><span>&nbsp;</span>Go to list'.html_safe, vehicle_contracts_path, {:title => "Cancel",:class => 'btn', "rel" => "tooltip", "data-placement" => "bottom"} %>
      </div>
    </div>
    <div class="breadcrumbs">
      <%= render_crumbs %>
      <div class="close-bread">
        <a href="#"><i class="icon-remove"></i></a>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <div id="vehicle-contract" class="box box-condensed box-color box-bordered">
          <%= render 'form' %>
        </div>
      </div>
    </div>    
  </div>
</div>