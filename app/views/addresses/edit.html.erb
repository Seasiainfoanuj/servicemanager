<%= render(:partial => 'layouts/left') %>
<div id="main">
  <div class="container-fluid">
    <%= render(:partial => 'layouts/notice') %>
    <div class="page-header">
      <div class="pull-left">
        <h1>Edit Address</h1>
      </div>
      <div class="pull-right">
        <%= link_to '<i class="icon-list-alt"></i>'.html_safe, addresses_path, {:title => "List All",:class => 'btn', "rel" => "tooltip", "data-placement" => "bottom"} %>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <div id="address-details" class="box box-condensed box-color box-bordered">
          <%= render 'form' %>
        </div>
      </div>
    </div>
  </div>
</div>
