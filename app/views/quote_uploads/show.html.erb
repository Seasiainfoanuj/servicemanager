<%= render(:partial => 'layouts/left') %>
<div id="main">
  <div class="container-fluid">
    <%= render(:partial => 'layouts/notice') %>
    <div class="row-fluid">
      <div class="span12">
        <div class="box box-condensed box-color box-bordered">
          <div class="box-title">
            <h3><i class="icon-file"></i> Files</h3>
          </div>
          <div class="box-content nopadding">
            <table class="table">
              <tr>
                <td><a href="<%= @quote_upload.upload.url(:original) %>"><%= image_tag @quote_upload.upload.url(:medium) %></a></td>
                <td>
                  <a href="<%= @quote_upload.upload.url(:original) %>" target="_blank"><%= @quote_upload.upload_file_name %></a>
                </td>
                <td class="align-right"><a class="btn" href="<%= @quote_upload.upload.url(:original) %>" target="_blank"><i class="icon-download-alt"></i></a></td>
              </tr>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
